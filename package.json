{
  "name": "meetup-management-system",
  "author": "Jan-Felix Abellera",
  "version": "0.0.0",
  "description": "A way to manage large-scale meetups for Tex Mechs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfabellera/meetup-management-system.git"
  },
  "scripts": {
    "install:backend": "cd ./backend/api && npm install && cd ../../",
    "install:frontend": "cd ./frontend && npm install && cd ../",
    "install:all": "npm run install:backend && npm run install:frontend",
    "build:backend": "cd ./backend/api && npm run build && cd ../../",
    "build:frontend": "cd ./frontend && npm run build && cd ../",
    "build:all": "npm run build:backend && npm run build:frontend",
    "style:all": "npx prettier --write ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "3.0.3",
    "typescript": "^5.2.2"
  }
}
